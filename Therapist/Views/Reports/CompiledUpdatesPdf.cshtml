                        <text>No</text>
                    }
                </p>
                <p>
                    <strong>@Html.DisplayNameFor(report => report.takingTurn):</strong>
                    @if (report.takingTurn == 1)
                    {
                        <text>Yes</text>
                    }
                    else if (report.takingTurn == 2)
                    {
                        <text>No</text>
                    }
                </p>
                <p>
                    <strong>@Html.DisplayNameFor(report => report.sharing):</strong>
                    @if (report.sharing == 1)
                    {
                        <text>Yes</text>
                    }
                    else if (report.sharing == 2)
                    {
                        <text>No</text>
                    }
                </p>
                <p>
                    <strong>@Html.DisplayNameFor(report => report.stayInGrouping):</strong>
                    @if (report.stayInGrouping == 1)
                    {
                        <text>Yes</text>
                    }
                    else if (report.stayInGrouping == 2)
                    {
                        <text>No</text>
                    }
                </p>
                <p>
                    <strong>@Html.DisplayNameFor(report => report.academicPerformance):</strong>
                    @if (report.academicPerformance == 1)
                    {
                        <text>Good</text>
                    }
                    else if (report.academicPerformance == 2)
                    {
                        <text>Average</text>
                    }
                    else if (report.academicPerformance == 3)
                    {
                        <text>Fair</text>
                    }
                    else if (report.academicPerformance == 4)
                    {
                        <text>Poor</text>
                    }
                    else if (report.academicPerformance == 5)
                    {
                        <text>Unable to Follow</text>
                    }
                </p>

                <p><strong>A - Analysis Problem, Short Term, Long Term Goal:</strong> @report.analysisProblem</p>
                <p><strong>P - Tx Done, Tx Plan:</strong> @report.txPlan</p>
                <p><strong>Therapist Incharge:</strong> @report.therapistIncharge</p>
            </div>
        }
    }
</body>
</html>
